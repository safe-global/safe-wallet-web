{
  "name": "web-core",
  "homepage": "https://github.com/5afe/web-core",
  "license": "MIT",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next dev",
    "build": "next build",
    "export": "next export",
    "lint": "tsc && next lint",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier -w \"{src,cypress,mocks,scripts}/**/*.{ts,tsx,css,js}\"",
    "test": "DEBUG_PRINT_LIMIT=30000 jest",
    "test:ci": "yarn test --ci --coverage --json --watchAll=false --testLocationInResults --runInBand --outputFile=jest.results.json",
    "cmp": "./scripts/cmp.sh",
    "routes": "node scripts/generate-routes.js > src/config/routes.ts && prettier -w src/config/routes.ts && cat src/config/routes.ts",
    "css-vars": "ts-node-esm ./scripts/css-vars.ts > ./src/styles/vars.css && prettier -w src/styles/vars.css",
    "generate-types:safeDeployments": "typechain --target ethers-v5 --out-dir src/types/contracts ./node_modules/@gnosis.pm/safe-deployments/dist/assets/**/*.json",
    "generate-types:spendingLimit": "typechain --target ethers-v5 --out-dir src/types/contracts ./node_modules/@gnosis.pm/safe-modules-deployments/dist/assets/**/*.json",
    "generate-types": "yarn generate-types:safeDeployments && yarn generate-types:spendingLimit",
    "postinstall": "yarn generate-types && yarn css-vars",
    "analyze": "ANALYZE=true yarn build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "yarn cypress:run --config baseUrl=http://localhost:8080 --spec cypress/e2e/smoke/*.cy.js",
    "serve": "npx -y http-server out"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@emotion/cache": "^11.10.1",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@gnosis.pm/safe-core-sdk": "^2.4.1",
    "@gnosis.pm/safe-deployments": "^1.15.0",
    "@gnosis.pm/safe-ethers-lib": "^1.4.0",
    "@gnosis.pm/safe-modules-deployments": "^1.0.0",
    "@gnosis.pm/safe-react-gateway-sdk": "^3.3.5",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.3",
    "@reduxjs/toolkit": "^1.8.2",
    "@sentry/react": "^7.8.1",
    "@sentry/tracing": "^7.8.1",
    "@web3-onboard/coinbase": "^2.0.10",
    "@web3-onboard/core": "^2.6.0",
    "@web3-onboard/fortmatic": "^2.0.9",
    "@web3-onboard/injected-wallets": "^2.0.15",
    "@web3-onboard/keepkey": "^2.1.7",
    "@web3-onboard/keystone": "^2.1.8",
    "@web3-onboard/ledger": "^2.1.7",
    "@web3-onboard/portis": "^2.0.7",
    "@web3-onboard/torus": "^2.0.8",
    "@web3-onboard/trezor": "^2.1.7",
    "@web3-onboard/walletconnect": "^2.0.8",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.1",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.6.8",
    "exponential-backoff": "^3.1.0",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "12.2.5",
    "papaparse": "^5.3.2",
    "qrcode": "^1.5.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.34.0",
    "react-papaparse": "^4.0.2",
    "react-qr-reader": "2.2.1",
    "react-redux": "^8.0.2",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@gnosis.pm/safe-core-sdk-types": "^1.4.0",
    "@next/bundle-analyzer": "^12.2.4",
    "@sentry/types": "^7.8.1",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.2",
    "@typechain/ethers-v5": "^8.0.0",
    "@types/jest": "^28.1.6",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "18.6.4",
    "@types/qrcode": "^1.4.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-qr-reader": "^2.1.4",
    "@types/semver": "^7.3.10",
    "cross-env": "^7.0.3",
    "cypress": "^10.6.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fuse.js": "^6.6.2",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.0",
    "ts-node": "^10.8.2",
    "typechain": "^8.0.0",
    "typescript": "4.7.4"
  }
}
